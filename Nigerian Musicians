CREATE TABLE nigerian_musicians (
    id INTEGER PRIMARY KEY AUTOINCREMENT, 
    stage_name TEXT,
    real_name TEXT
); 
INSERT INTO nigerian_musicians (stage_name, real_name) VALUES ("davido", "david adeleke");
