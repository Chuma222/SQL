CREATE TABLE songs (
    id INTEGER PRIMARY KEY AUTOINCREMENT, 
    artist_id INTEGER,
    title TEXT,
    release_year INTEGER,
    views NUMERIC
); 
INSERT INTO songs (artist_id, title, release_year, views) 
